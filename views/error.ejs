<!doctype html>
<html lang="en">

<head> 
<%
    var root = 'https://hadith.quranunlocked.com';
    var site = {
      name: 'Ḥadīth Unlocked',
      url: root,
      logo: `${root}/images/logo.png`,
      admin: false,
    };
    
    var subtitle_en = `Error ${res.statusCode} `;
    var page = {
      menu: 'Recent',
      title_en: `${site.name} | Error: ${res.statusCode} ${utils.emptyIfNull(message)} `,
      subtitle_en: subtitle_en,
      subtitle: null,
      canonical: '/',
      context: {},
    };
%> 

<%- include('sub-views/head.ejs', { site: site, page: page }); %> 

</head>

<body>

  <%- include('sub-views/header.ejs', { site: site, page: page }); %> 

  <div class="container col-lg-8 mx-auto">
    
    <main class="search-results">
      <div class="row"> 
        
		<h5><%- res.statusCode %> <%- message %></h5>
		<% if (res.statusCode >= 500) { %>
			<pre><%- error.stack %></pre>
		<% } %>

      </div>
    </main>
  </div>

<%- include('sub-views/footer.ejs'); %>
<%- include('sub-views/scripts.ejs', { site: site }); %>

</body>
</html>
